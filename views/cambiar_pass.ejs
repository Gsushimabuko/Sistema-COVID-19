  <body class="text-center">   

    <%- include('menu') %>
    
    <main class="form-signin">
      <form method="post" action="/cambiar_pass">
        

        <img class="mb-4 pr-2" src="data:image/png;base64," width="200" height="200">
        <h4 class="h3 mb-3 fw-normal header1"> 
        <b> CAMBIOS DE CONTRASEÑA </b></h4>

        <div class="form-check">
          <input class="form-check-input" type="radio" name="username" value="sistema" id="flexRadioDefault1">
          <label class="form-check-label" for="flexRadioDefault1">
            SISTEMA
          </label>
        </div>
        <div class="form-check">
          <input class="form-check-input" type="radio" name="username" value="directivo" id="flexRadioDefault2">
          <label class="form-check-label" for="flexRadioDefault2">
            DIRECTIVO
          </label>
        </div>
        <div class="form-check">
          <input class="form-check-input" type="radio" name="username" value="salud" id="flexRadioDefault3">
          <label class="form-check-label" for="flexRadioDefault3">
            SALUD
          </label>
        </div>

        <div class="form-check">
          <input class="form-check-input" type="radio" name="username" value="guardia" id="flexRadioDefault3">
          <label class="form-check-label" for="flexRadioDefault3">
            GUARDIA
          </label>
        </div>

        <div class="form-floating">
          <input type="password" name='password1' class="form-control" id="floatingInput" placeholder="Contraseña">
          <label for="floatingInput">Contraseña</label>
        </div>
        <div class="form-floating">
          <input type="password" name='password2'  class="form-control" id="floatingPassword" placeholder="Contraseña">
          <label for="floatingPassword">Contraseña otra vez</label>
        </div>

        <button class="w-100 btn btn-lg btn-primary" type="submit">Guardar Contraseña</button>

      </form>
    </main>

  </body>
</html>
